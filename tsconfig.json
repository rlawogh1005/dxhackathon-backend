{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json", "composite": true },
    { "path": "./tsconfig.node.json", "composite": true }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  },
  "include": ["src", "src/types/**/*.d.ts"]
}
